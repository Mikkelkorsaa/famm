@inject NavigationManager navigationmanager
<div class="dashboardButton" @onclick="()=>OnButtonClick(myProfileUrl)" >
	Se min Profil
</div>
<div class="dashboardButton" @onclick="()=>OnButtonClick(submitAbstractUrl)">
	Indsend Abstrakt
</div>

@code {
	private string myProfileUrl = "/profile";
	private string submitAbstractUrl = "/upload-abstract";

	private void OnButtonClick(string url)
	{
		navigationmanager.NavigateTo(url);
	}

}